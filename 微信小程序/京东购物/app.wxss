.jd-label {
    position: absolute;
    padding-right: 5px;
    color: white;
    line-height: 20px;
    font-size: 12px;
    background-image: linear-gradient(to right,#e93b3d,#ff9574);
    border-radius: 0 10px 10px 0;
    box-shadow: 0 4px 6px rgba(233,59,61,.2);
}

.jd-label::before,.jd-label::after {
    content: "";
    position: absolute;
    left: -5px;
    width: 5px;
    border-radius: 5px 0 0 5px;
}

.jd-label::before {
    top: 0;
    height: 28px;
    background-color: #e93b3d;
}

.jd-label::after {
    top: 20px;
    height: 8px;
    background-color: #d53f41;
}

.tag {
    position: absolute;
    padding: 0 6px;
    color: white;
    line-height: 18px;
    font-size: 12px;
    background-image: linear-gradient(to right,#e93b3d,#ff9574);
    box-shadow: 0 4px 6px rgba(233,59,61,.2);
}

.btn_grad {
    width: 80px;
    line-height: 25px;
    color: white;
    background-image: linear-gradient(to right,#e93b3d,#ff9574);
    border-radius: 999px;
    box-shadow: 0 6px 10px rgba(233,59,61,.2);
}

.btn_grad--disabled {
    opacity: .3;
}
