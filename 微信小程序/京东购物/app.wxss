@font-face {
    font-family: "font_steelfish";
    src: url('https://wq.360buyimg.com/data/ppms/others/steelfishRg.svg') format("truetype");
    font-style: normal;
    font-weight: normal;
}

page {
    font-size: 14px;
    line-height: 1.5;
    color: #333;
    background-color: #f7f7f7;
}

page::before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 0;
    border-bottom: 1rpx solid #ddd;
    z-index: 100;
}

scroll-view {
    overflow: hidden;
}

.line1 {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.line2,.line3 {
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.line3 {
    -webkit-line-clamp: 3;
}

.btn_disabled {
    color: #999;
    background-color: #ccc;
}

.back2top {
    position: fixed;
    display: block;
    right: 0;
    bottom: 40px;
    width: 45px;
    height: 45px;
    background-color: rgba(0,0,0,.7);
    border-radius: 4px 0 0 4px;
}

.back2top::after {
    content: "";
    display: block;
    width: 100%;
    height: 100%;
    background-image: url("data:image/png;base64,");
    background-position: center;
    background-repeat: no-repeat;
    background-size: 16px 20px;
}

.fixed_return {
    position: fixed;
    right: 0;
    bottom: 85px;
    width: 45px;
    height: 45px;
    background-color: rgba(0,0,0,.7);
    border-radius: 4px 0 0 4px;
}

.fixed_return::before {
    content: "";
    display: block;
    width: 45px;
    height: 32px;
    color: white;
    font-size: 11px;
    text-align: center;
    background-image: url('https://img11.360buyimg.com/jdphoto/s80x80_jfs/t19345/2/453803839/1301/1a09aa7e/5a7916edNb95aedac.png');
    background-position: center;
    background-repeat: no-repeat;
    background-size: 20px 20px;
}

.fixed_return::after {
    content: '京东首页';
    position: relative;
    display: block;
    top: -2px;
    font-size: 8px;
    text-align: center;
    color: #f7f7f7;
}

.fixed_return.cate_text::after {
    content: "返回分类";
    width: 26px;
    height: 26px;
    line-height: 13px;
    background-image: none;
}

.flag_solid,.flag_hollow {
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    box-sizing: border-box;
    padding: 2px 3px;
    line-height: 1;
    font-size: 10px;
    border-radius: 2px;
}

.flag_solid {
    color: #fff;
    background: #E93B3D;
}

.flag_solid.green {
    background: #09bb07;
}

.flag_quality {
    position: relative;
    width: 86rpx;
    height: 28rpx;
    line-height: 28rpx;
    vertical-align: middle;
    font-size: 20rpx;
    color: #6400c6;
    background: #fff;
    border: 1rpx solid #6400c6;
    border-radius: 4rpx;
    padding-left: 36rpx;
}

.flag_quality:before {
    position: absolute;
    left: 0;
    top: 0;
    content: "";
    width: 32rpx;
    height: 30rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-color: #6400c6;
    background-size: 100% 100%;
}

.flag_hollow {
    color: #E93B3D;
    background: #fff;
    border: 1rpx solid #E93B3D;
}

.reset_btn {
    margin: 0;
    padding: 0;
    font-size: inherit;
    border-radius: 0;
    color: #333;
    background-color: transparent;
}

.reset_btn::after {
    border: 0;
}

.x_bg {
    background: url('https://img11.360buyimg.com/jdphoto/s100x100_jfs/t15085/104/1569475082/1026/e5cd0ac0/5a5319a2Nae505852.png') no-repeat center center;
    background-size: 80px;
}

