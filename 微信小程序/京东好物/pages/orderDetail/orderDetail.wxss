    background-color: #fff;
    padding-left: 66rpx;
    padding: 22rpx 0 22rpx 66rpx;
}

.user-info .first-line {
    height: 28rpx;
    line-height: 28rpx;
    margin-bottom: 16rpx;
}

.user-info .first-line .lt {
    font-size: 28rpx;
    color: #000;
