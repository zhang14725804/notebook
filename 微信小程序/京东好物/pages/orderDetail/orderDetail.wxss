}

.user-info .icn-user-head {
    position: absolute;
    top: 22rpx;
    left: 21rpx;
    width: 30rpx;
    height: 30rpx;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.user.png') 0 0 no-repeat;
    background-size: 30rpx 30rpx;
}

.commodity-list {
    margin-top: 18rpx;
}

.commodity-list .shop-info {
    position: relative;
    height: 60rpx;
    background-color: #fff;
    box-sizing: border-box;
    border-bottom: 2rpx solid #efefef;
}

.commodity-list .shop-info .icn-shop {
    position: absolute;
    top: 19rpx;
    left: 21rpx;
    width: 34rpx;
    height: 24rpx;
    background: url(http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.shop.png) 0 0 no-repeat;
    background-size: 34rpx 24rpx;
}

.commodity-list .shop-info .icn-pop-shop {
    position: absolute;
    top: 17rpx;
    left: 21rpx;
    width: 29rpx;
    height: 26rpx;
    background: url(http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.pop.shop.png) 0 0 no-repeat;
    background-size: 29rpx 26rpx;
}

.commodity-list .shop-info text {
    display: block;
    padding-left: 66rpx;
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #353535;
    box-sizing: border-box;
}

.commodity-list .shop-info .go-to-customer-service {
    position: absolute;
    right: 28rpx;
    top: 10rpx;
}

.commodity-list .shop-info .go-to-customer-service .cart-text {
    float: left;
    font-size: 28rpx;
}

.commodity-list .shop-info .go-to-customer-service icon {
    float: left;
    width: 32rpx;
    height: 32rpx;
    position: relative;
    top: 5rpx;
    margin-right: 15rpx;
}

.commodity-list .shop-info .icon-pop-service {
    background: url('http://newbuz.360buyimg.com/jingshop/orderDetail/icon.service.pop.png') 0 0 no-repeat;
    background-size: 32rpx 32rpx;
}

.commodity-list .shop-info .icon-service {
    background: url('http://newbuz.360buyimg.com/jingshop/orderDetail/icon.service.jd.png') 0 0 no-repeat;
    background-size: 32rpx 32rpx;
}

.commodity-list .list-ul {
    padding-left: 20rpx;
    background-color: #fff;
}

.commodity-list .list-ul .list-li {
    height: 200rpx;
    padding: 20rpx 28rpx 20rpx 0;
    box-sizing: border-box;
    border-bottom: 1rpx solid #eaeaea;
}

.commodity-list .list-ul .list-li:last-child {
    border-bottom-width: 0;
}

.commodity-list .list-ul .list-li .com-img {
    float: left;
    width: 160rpx;
    height: 160rpx;
    border: 1rpx solid #e8e8e8;
    border-radius: 4rpx;
    margin-right: 24rpx;
    box-sizing: border-box;
}

.commodity-list .list-ul .list-li .com-img image {
    width: 100%;
    height: 100%;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/default.img.png') 0 0 no-repeat;
    background-size: 100% 100%;
}

.commodity-list .list-ul .list-li .rt-com-info {
    float: left;
    width: 518rpx;
    height: 100%;
    padding-top: 24rpx;
    padding-bottom: 27rpx;
    box-sizing: border-box;
}

.commodity-list .list-ul .list-li .rt-com-info .first-line {
    height: 52rpx;
    position: relative;
}

.commodity-list .list-ul .list-li .rt-com-info .first-line .wname {
    height: 52rpx;
    display: inline-block;
    font-size: 26rpx;
    color: #000;
    padding-right: 166rpx;
    line-height: 26rpx;
    vertical-align: middle;
    display: -webkit-box;
    -webkit-box-align: start;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.commodity-list .list-ul .list-li .rt-com-info .first-line .price {
    position: absolute;
    right: 0;
    top: 0;
    font-size: 26rpx;
    color: #000;
}

.commodity-list .list-ul .list-li .rt-com-info .second-line {
    height: 28rpx;
    line-height: 28rpx;
    margin-top: 22rpx;
}

.commodity-list .list-ul .list-li .rt-com-info .second-line text {
    font-size: 28rpx;
    color: #888;
}

.more-wares-area {
