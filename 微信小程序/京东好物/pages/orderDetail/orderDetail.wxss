.order-process .cancel-status .second-line {
    font-size: 24rpx;
    color: #353535;
}

.order-process .cancel-status .third-line {
    font-size: 24rpx;
    color: #888;
}

.order-process .cancel-status .icn-status {
    position: absolute;
    top: 22rpx;
    left: 21rpx;
    width: 30rpx;
    height: 30rpx;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.status.png') 0 0 no-repeat;
    background-size: 30rpx 30rpx;
}

.order-process .tracking {
    position: relative;
    overflow: hidden;
    padding-left: 66rpx;
    padding-right: 60rpx;
    padding-top: 20rpx;
}

.order-process .cancel-status view,.order-process .tracking view {
    margin: 20rpx 0;
}

.order-process .cancel-status view:first-child,.order-process .tracking view:first-child {
    margin-top: 0;
}

.order-process .tracking .icon-tracking {
    position: absolute;
    top: 22rpx;
    left: 21rpx;
    width: 30rpx;
    height: 30rpx;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.tracking.png?v=333') 0 0 no-repeat;
    background-size: 30rpx 30rpx;
}

.user-info {
