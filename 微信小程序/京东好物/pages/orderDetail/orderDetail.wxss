}

.order-info .basic .line .rt {
    font-size: 28rpx;
    color: #000;
}

.order-info .basic .order-state {
    position: absolute;
    top: 50%;
    right: 28rpx;
    font-size: 28rpx;
    color: #f23030;
    height: 28rpx;
    line-height: 28rpx;
    margin-top: -14rpx;
}

.order-process {
    position: relative;
    background-color: #fff;
    margin-top: 18rpx;
}

.order-process .first-line {
    font-size: 28rpx;
    color: #000;
}

.order-process .second-line {
    font-size: 24rpx;
    color: #888;
}

.order-process .icon-right-arrow {
    position: absolute;
    right: 28rpx;
    top: 50%;
    width: 14rpx;
    height: 24rpx;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.right.arrow.png') 0 0 no-repeat;
    background-size: 14rpx 24rpx;
