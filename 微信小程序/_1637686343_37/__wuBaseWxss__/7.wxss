    max-height: 76rpx;
    margin-bottom: 11rpx;
    display: -webkit-box;
    overflow: hidden;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
}

.m-list-liveVideo_text_info {
    font-size: 22rpx;
    line-height: 22rpx;
    overflow: hidden;
    white-space: nowrap;
