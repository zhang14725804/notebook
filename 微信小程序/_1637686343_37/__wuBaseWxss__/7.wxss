}

.m-list-liveVideo_content:first-child {
    padding: 0 30rpx 30rpx;
}

.m-list-liveVideo_imgBox {
    width: 220rpx;
    height: 124rpx;
    position: relative;
    margin-right: 28rpx;
}

.m-list-liveVideo_text {
    flex: 1;
    box-sizing: border-box;
    text-overflow: ellipsis;
    overflow: hidden;
    position: relative;
}

.m-list-liveVideo_text_title {
    font-size: 28rpx;
    line-height: 38rpx;
