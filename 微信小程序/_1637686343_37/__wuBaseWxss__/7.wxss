.m-list {
    width: 100%;
    border-bottom: 1px solid #f0f0f0;
    overflow-x: hidden;
}

.m-list_content {
    display: flex;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-left: -3rpx;
}

.m-list_item {
    padding-left: 3rpx;
    width: 33.3333%;
    box-sizing: border-box;
}

.m-list_item-pic {
    position: relative;
    width: 100%;
    height: 330rpx;
    overflow: hidden;
}

.m-list_item-pic_image {
    width: 100%;
    height: 100%;
}

.m-list_c-rb {
    width: 100%;
    height: 80rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: linear-gradient(to top,rgba(0,0,0,0.5),rgba(0,0,0,0));
}

.m-list_c-date {
    position: absolute;
    top: 41rpx;
    right: 20rpx;
    color: white;
    font-size: 21rpx;
}

.m-list-picTitle {
    max-height: 76rpx;
    overflow: hidden;
    font-size: 28rpx;
    line-height: 38rpx;
    margin: 20rpx 20rpx 35rpx 20rpx;
}

.m-list-horizontal_content {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-left: -2rpx;
}

.m-list-horizontal_item {
    width: 374rpx;
    margin-left: 2rpx;
}

.m-list-horizontal_item-pic {
    position: relative;
    width: 100%;
    height: 211rpx;
    overflow: hidden;
}

.m-list-text {
    padding: 15rpx 20rpx;
    border-top: 1px solid #f0f0f0;
}

.m-list-text-items {
    display: flex;
}

.m-list-text-items-title {
    font-size: 28rpx;
    color: #333;
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 78rpx;
}

.m-list-text-items-time {
    font-size: 24rpx;
    color: #999;
    line-height: 78rpx;
}

.m-list-text-items-nolog {
    font-size: 28rpx;
    color: #999;
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 78rpx;
}

.m-pic-text {
    padding: 40rpx 20rpx;
    width: 100%;
    box-sizing: border-box;
}

.m-pic-text-content {
    display: flex;
    width: 100%;
    box-sizing: border-box;
}

.m-pic-text-content-img {
    width: 230rpx;
    height: 307rpx;
    margin-right: 20rpx;
    position: relative;
}

.m-pic-text-content-img_image {
    width: 230rpx;
    height: 307rpx;
}

.m-pic-text-content-details {
    flex: 1;
    position: relative;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding-right: 45rpx;
}

.m-pic-text-content-details_title {
    font-size: 28rpx;
    margin-bottom: 8rpx;
    line-height: 1;
    padding-top: 5rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.m-pic-text-content-details_info {
    font-size: 22rpx;
    color: #555;
    line-height: 32rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.m-pic-text-content-details_btn {
    font-size: 24rpx;
    color: #fff;
    line-height: 70rpx;
    padding: 0 34rpx;
    background-color: #00c000;
    width: 190rpx;
    box-sizing: border-box;
    border-radius: 70rpx;
    position: absolute;
    bottom: 0;
}

.m-numList {
    padding: 0 20rpx 40rpx 20rpx;
    width: 100%;
    box-sizing: border-box;
}

.m-numList-content {
    width: 100%;
    box-sizing: border-box;
}

.m-numList-content_items {
    width: 130rpx;
    height: 110rpx;
    display: inline-block;
    background-color: #f5f5f5;
    margin-right: 15rpx;
    text-align: center;
    font-size: 28rpx;
    line-height: 110rpx;
    position: relative;
}

.m-numList-content_items:last-child {
    margin-right: 0;
}

.m-titleList {
    padding: 0 20rpx;
    width: 100%;
    box-sizing: border-box;
}

.m-titleList-content {
    width: 100%;
    box-sizing: border-box;
}

.m-titleList-content_items {
    width: 100%;
    height: 90rpx;
    display: inline-block;
    background-color: #f5f5f5;
    margin-bottom: 15rpx;
    font-size: 28rpx;
    line-height: 90rpx;
    position: relative;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.m-titleList-content_items:last-child {
    margin-bottom: 0;
}

.m-topList {
    width: 100%;
    height: 228rpx;
    padding: 40rpx 20rpx;
    border-bottom: 1px solid #f0f0f0;
    box-sizing: border-box;
}

.m-topList_content {
    width: 100%;
    display: flex;
}

.m-topList_imgBox {
    width: 258rpx;
    height: 146rpx;
    position: relative;
    margin-right: 30rpx;
}

.m-topList_img {
    width: 100%;
    height: 100%;
}

.m-topList_text {
    flex: 1;
    box-sizing: border-box;
    text-overflow: ellipsis;
    overflow: hidden;
    position: relative;
}

.m-topList_text_title {
    font-size: 28rpx;
    line-height: 38rpx;
    word-break: break-word;
    margin-top: 2rpx;
    color: #333;
    max-height: 76rpx;
    margin-bottom: 11rpx;
    display: -webkit-box;
    overflow: hidden;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
}

.m-topList_text_info {
    font-size: 22rpx;
    line-height: 28rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #666;
    position: absolute;
    bottom: 0;
    width: 100%;
}

.m-topList_text_c-info {
    display: block;
    margin-top: 3rpx;
    margin-bottom: 5rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.m-topList-history {
    width: 100%;
    height: 154rpx;
    padding: 0rpx 20rpx 42rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.m-topList-history_content {
    width: 100%;
    display: flex;
    height: 100%;
}

.m-topList-history_check {
    width: 40rpx;
    height: 100%;
    position: relative;
    margin-right: 20rpx;
}

.m-topList-history_check_icon {
    width: 100%;
    height: 40rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.m-topList-history_imgBox {
    width: 200rpx;
    height: 100%;
    position: relative;
    margin-right: 20rpx;
}

.m-topList-history_img {
    width: 100%;
    height: 112rpx;
}

.m-topList-history_text {
    flex: 1;
    box-sizing: border-box;
    text-overflow: ellipsis;
    overflow: hidden;
    position: relative;
}

.m-topList-history_text_title {
    font-size: 28rpx;
    line-height: 38rpx;
    word-break: break-word;
    margin-top: 2rpx;
    color: #333;
    max-height: 76rpx;
    margin-bottom: 11rpx;
    display: -webkit-box;
    overflow: hidden;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
}

.m-topList-history_text_info {
    font-size: 22rpx;
    line-height: 28rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #999;
    position: absolute;
    bottom: 0;
    width: 100%;
}

.m-topList-history_text_c-info {
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.m-list-liveVideo {
    border-bottom: 2rpx solid #f0f0f0;
}

.m-list-liveVideo_content {
    width: 100%;
