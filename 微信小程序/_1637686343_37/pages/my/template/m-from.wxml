<template name="m-title_subscribe">
    <view bindtap="findSubscribeMore" class="m-title">
        <text class="m-title_h">我的收藏</text>
        <view class="m-title_more">
            <image class="c-icon-more" height="15" src="/resource/images/c-icon-arrow.png" width="15"></image>
        </view>
    </view>
</template>
<template name="m-title">
    <form bindsubmit="findmore" reportSubmit="true">
        <button class="m-title button-in-form" formType="submit">
            <text class="m-title_h">播放历史</text>
            <view class="m-title_more">
                <image class="c-icon-more" height="15" src="/resource/images/c-icon-arrow.png" width="15"></image>
            </view>
        </button>
    </form>
</template>
<template name="m-from-user-contact">
    <form bindsubmit="feedback" class="m-form_common" reportSubmit="true">
        <button class="m-from-user-contact button-in-form" formType="submit">
            <button class="m-from-user-contact_btn">设置</button>
            <view class="m-from-user-contact_link">
                <image class="c-icon-more" height="15" src="/resource/images/c-icon-arrow.png" width="15"></image>
            </view>
        </button>
    </form>
</template>
