.hide {
    display: none;
}

.complaint-container {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    text-align: center;
    background-color: #f0f0f0;
}

.complaint-container .kefu-cont {
    height: 50px;
    line-height: 50px;
    background: #fff;
    font-size: 14px;
    border-radius: 2px;
}

.complaint-container .kefu-cont .btn-kefu {
    width: 50%;
    text-align: center;
    float: left;
    position: relative;
}

.complaint-container .kefu-cont .btn-tel:before {
    display: block;
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    height: 100%;
    border-left: 1px solid #eee;
    transform: scaleX(0.5);
    -webkit-transform: scaleX(0.5);
}

.complaint-container .footer.kefu-cont .btn-tel:before {
    border: 0;
}

.complaint-container .kefu-cont text {
    display: inline-block;
    text-decoration: none;
    padding-left: 18px;
    color: #666;
}

.complaint-container .kefu-cont .kefu-online {
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 14px auto;
}

.complaint-container .kefu-cont .kefu-tel {
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 14px auto;
}

.complaint-container #complaint-done {
    top: 0;
    left: 100%;
    z-index: 100;
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.complaint-container #complaint-done.right-to-left {
    left: 0;
    transition: left 0.5s 0.1s ease-in-out;
    -webkit-transition: left 0.5s 0.1s ease-in-out;
}

.complaint-container .outer {
    min-height: 100%;
}

.complaint-container #inner {
    margin: 0 53px;
    padding: 27px 0 58px;
}

.complaint-container #complaint-done .footer {
    margin-top: -58px;
    height: 58px;
    line-height: 58px;
}

.complaint-container #complaint-done .footer text {
    display: inline-block;
    border: 0;
    float: none;
}

.complaint-container #complaint-done .footer text.kefu-tel {
    margin-left: 90px;
}

.complaint-container #complaint-done .h1 {
    padding-top: 20px;
    font-weight: 400;
    color: #666;
    font-size: 40px;
}

.complaint-container #content {
    position: relative;
}

.complaint-container #list {
    margin-left: 15px;
}

.complaint-container #list .li {
    margin-top: 22px;
    font-size: 12px;
    text-align: left;
    position: relative;
}

.complaint-container #list .li:before {
    width: 6px;
    height: 6px;
    content: '';
    display: block;
    background: #e3e3e3;
    border-radius: 100%;
    position: absolute;
    top: 2px;
    left: -15px;
}

.complaint-container #list .title {
    padding-bottom: 3px;
    line-height: 1;
    color: #333;
}

.complaint-container #list .item {
    line-height: 18px;
    color: #878787;
    word-wrap: break-word;
    word-break: break-all;
}